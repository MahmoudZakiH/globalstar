<odoo>
    <data>

        <record id="stock_picking_type_user_global_rule" model="ir.rule">
            <field name="name">Stock Picking Type User GlobalStar</field>
            <field name="model_id" ref="stock.model_stock_picking_type"/>
            <field eval="True" name="global"/>
            <field name="domain_force">[('warehouse_id', 'in', user.stock_warehouse_ids.ids)]</field>
            <field name="groups" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>

        <record id="stock_picking_type_admin_global_rule" model="ir.rule">
            <field name="name">Stock Picking Type Admin GlobalStar</field>
            <field name="model_id" ref="stock.model_stock_picking_type"/>
            <field eval="True" name="global"/>
            <field name="domain_force">[(1, '=', 1)]</field>
            <field name="groups" eval="[(4, ref('stock.group_stock_manager'))]"/>
        </record>

        <record id="contact_user_global_rule" model="ir.rule">
            <field name="name">Contacts User GlobalStar</field>
            <field name="model_id" ref="stock.model_res_partner"/>
            <field name="domain_force">['|', ('user_id', '=', user.id), ('user_ids', '!=', False)]</field>
            <field name="groups" eval="[(4, ref('globalstar_security.group_contacts_user_global'))]"/>
        </record>

        <record id="contact_manager_global_rule" model="ir.rule">
            <field name="name">Contacts Manager GlobalStar</field>
            <field name="model_id" ref="stock.model_res_partner"/>
            <field name="domain_force">['|', ('team_id', 'in', user.crm_team_ids.ids), ('user_ids', '!=', False)]</field>
            <field name="groups" eval="[(4, ref('globalstar_security.group_contacts_manager_global'))]"/>
        </record>

        <record id="contact_admin_global_rule" model="ir.rule">
            <field name="name">Contacts Admin GlobalStar</field>
            <field name="model_id" ref="stock.model_res_partner"/>
            <field name="domain_force">[(1, '=', 1)]</field>
            <field name="groups" eval="[(4, ref('globalstar_security.group_contacts_admin_global'))]"/>
        </record>

    </data>
</odoo>